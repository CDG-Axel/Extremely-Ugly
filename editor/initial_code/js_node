import assert from "assert";

function ugly_number(n: number): number {
    // your code here
    return n;
}

console.log('Example:');
console.log(ugly_number(4));

// These "asserts" are used for self-checking
assert.equal(ugly_number(4), 4);
assert.equal(ugly_number(6), 6);
assert.equal(ugly_number(11), 15);
assert.equal(ugly_number(999), 51018336);

